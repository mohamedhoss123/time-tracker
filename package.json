 "version": "0.0.0",
 "type": "commonjs",
 "scripts": {
  "elec:start": "electron ./electron/main.js",
  "elec:build": "electron-builder build",
  "dev": "vite",
  "build": "vite build",
  "preview": "vite preview"
 },
