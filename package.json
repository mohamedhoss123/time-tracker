  "react": "^18.2.0",
  "react-dom": "^18.2.0"
 },
 "devDependencies": {
  "electron-builder": "^23.6.0",
  "electron": "^22.2.0",
  "@types/react": "^18.0.27",
  "@types/react-dom": "^18.0.10",
  "@vitejs/plugin-react-swc": "^3.0.0",
  "vite": "^4.1.0"
 },
 "main": "./electron/main.js",
 "build": {
  "directories": {
   "output": "releases/${platform}/${arch}"
  },
  "win": {
   "target": [
    {
     "target": "nsis",
     "arch": [
      "x64"
     ]
    }
   ]
